# RapidAPI Configuration for PropertyFinder
# Get your API key from https://rapidapi.com
# Subscribe to "UAE Real Estate API - PropertyFinder.ae Data"
RAPIDAPI_KEY=your_rapidapi_key_here

# Feature Flag: Enable real-time data from PropertyFinder API
# Set to "true" to use PropertyFinder API, "false" to use CSV
USE_REALTIME_DATA=false

# Fallback Configuration
# If PropertyFinder API fails, fall back to CSV data
FALLBACK_TO_CSV=true

# Optional: If the API host is different, override it here
# Get this from the RapidAPI PropertyFinder API page (x-rapidapi-host header value)
RAPIDAPI_HOST=market-data-point1-market-data-point-default-api.p.rapidapi.com

# API Rate Limit Protection
# Maximum number of results per API request (default: 10)
# Lower values = fewer API calls = slower rate limit usage
RAPIDAPI_MAX_RESULTS=10

# Cache duration in minutes (default: 30)
# Longer cache = fewer API calls
RAPIDAPI_CACHE_MINUTES=30
